# Enable RewriteEngine
RewriteEngine On

# Block access to specific PHP files
<FilesMatch "^(auth\.php|permissions\.php|file2\.php|file3\.php)$">
    Require all denied
</FilesMatch>

# Block access to specific directories: zzz, images, zbackup
RewriteRule ^(zzz|images|zbackup)/ - [F,L]

# RewriteEngine On

